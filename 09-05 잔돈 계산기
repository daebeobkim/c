#include <stdio.h>
#define _CRT_SECURE_NO_WARNINGS

int main(void)
{
	int a, b, c, d;
	printf("투입한 돈\n");
	scanf("%d", &a);
	printf("물건 값\n");
	scanf("%d", &b);
	c = a - b;
	printf("거스름돈 %d\n",c);

	printf("100원 동전의 개수: %d\n",c / 100);
	d = c / 100;
	printf("10원 동전의 개수: %d\n", ((c%100)/10));

	return 0;
}
