#include <stdio.h>
#define _CRT_SECURE_NO_WARNINGS

int main(void)
{
	int a, b, c, d;
	printf("연도를 입력하시외:");
	scanf("%d", &a);
	if (a % 4 == 0)
		printf("%d년은 윤년입니다", a);
	else if (a % 100 != 0)
		printf("%d년은 윤년이 아닙니다", a);
	else if (a % 400 == 0)
		printf("%d년은 윤년입니다", a);
	else
		printf("%d년은 윤년이 아닙니다", a);
	return 0;
}
