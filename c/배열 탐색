#include <stdio.h>
#define _CRT_SECURE_NO_WARNINGS
#define SIZE 9

int main(void)
{
	int search;
	int x[SIZE] = {10,20,30,40,50,60,70,80,90};
	int i;
	for (i = 0; i < SIZE; i++)
		printf("%d ", x[i]);
	printf("탐색할 값을 입력하시오:\n");
	scanf("%d", &search);
	for (i = 0; i < SIZE; i++) {
		if (x[i] == search)
			printf("%d", i);
	}
	return 0;
}
